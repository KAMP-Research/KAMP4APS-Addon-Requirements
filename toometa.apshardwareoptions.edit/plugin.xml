<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated apshardwareoptions -->
      <factory
            uri="http://www.example.org/apshardwareoptions"
            class="apshardwareoptions.provider.ApshardwareoptionsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated apshardwareoptions -->
      <extender
            uri="http://www.example.org/apsoptions"
            class="apshardwareoptions.provider.ApshardwareoptionsItemProviderAdapterFactory$ApsoptionsChildCreationExtender"/>
      <extender
            uri="http://sdq.ipd.kit.edu/QML/dimensiontypes/1.0"
            class="apshardwareoptions.provider.ApshardwareoptionsItemProviderAdapterFactory$DimensiontypesChildCreationExtender"/>
   </extension>

</plugin>
